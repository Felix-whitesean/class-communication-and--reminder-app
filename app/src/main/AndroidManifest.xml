			android:exported="true">
		</activity>
		<activity
			android:name=".general_dashboard__2_activity"
			android:configChanges="orientation|keyboardHidden|screenSize"
			android:label="Test page"
			android:exported="true">
		</activity>
		<activity
			android:name=".LoginActivity"
			android:configChanges="orientation|keyboardHidden|screenSize"
